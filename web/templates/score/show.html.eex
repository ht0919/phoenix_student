<h2>詳細</h2>

<ul>

  <li>
    <strong>試験日:</strong>
    <%= @score.date %>
  </li>

  <li>
    <strong>学生氏名:</strong>
    <%= @score.student.name %>
  </li>

  <li>
    <strong>受験科目:</strong>
    <%= @score.subject.name %>
  </li>

  <li>
    <strong>点数:</strong>
    <%= @score.point %>
  </li>

</ul>

<span><%= link "編集", to: score_path(@conn, :edit, @score) %></span>
<span><%= link "戻る", to: score_path(@conn, :index) %></span>
